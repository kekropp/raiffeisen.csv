<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Raiffeisen.CSV</Title>
        <Authors>Kenny Kropp &lt;kenny@kropp.io&gt;</Authors>
        <Description>A CSV Parser for Raiffeisen Exports</Description>
        <PackageProjectUrl>https://github.com/kekropp/raiffeisen.csv</PackageProjectUrl>
        <RepositoryUrl>https://github.com/kekropp/raiffeisen.csv</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
    </PropertyGroup>

</Project>
